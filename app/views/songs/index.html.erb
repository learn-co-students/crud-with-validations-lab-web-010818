<h1>Songs</h1>
<table>
  <tbody>
    <% @songs.each do |song| %>
    <tr>
      <td><%= song.id %></td>
      <td><%= song.title %></td>
      <td><%= song.artist_name %></td>
      <td><%= song.genre %></td>
      <td><%= song.released %></td>
      <td><%= song.release_year %></td>
       <td>
         <%= link_to "View", song %> |
         <%= link_to "Edit", edit_song_path(song) %> |
         <%= link_to "DELETE", delete_song_path(song) %>
       </td>
      </tr>
    <% end %>
  </tbody>
</table>
